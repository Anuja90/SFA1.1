<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 
<hibernate-mapping>
 
    <class name="com.persistence.SalaryDeduction" table="tblDeduction">
 
        <id name="deduct_id" column="deduct_id">
            <generator class="native" />
        </id>
 
        <property name="emp_id">
            <column name="emp_id" length="16" not-null="true" />
        </property>
 
        <property name="amount">
            <column name="amount" length="16" not-null="true" />
        </property>
        
        
         <property name="deductionname">
            <column name="deductionname" length="16" not-null="true" />
        </property>
        
         <property name="addDate">
            <column name="addDate" length="16" not-null="true" />
        </property>
        
    </class>
</hibernate-mapping>