<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 
<hibernate-mapping>
 
    <class name="com.persistence.salary" table="tblSalary">
 
        <id name="salary_id" column="salary_id">
            <generator class="native" />
        </id>
 
        <property name="basic_salary">
            <column name="basic_salary" length="16" not-null="true" />
        </property>
 
        <property name="epf_rate">
            <column name="epf_rate" length="16" not-null="true" />
        </property>
        
        <property name="welfare">
            <column name="welfare" length="16" not-null="true" />
        </property>
        
        <property name="payetax">
            <column name="payetax" length="16" not-null="true" />
        </property>
        
    </class>
</hibernate-mapping>